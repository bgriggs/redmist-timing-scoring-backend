{
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "console": {
        "type": "Console",
        "layout": "${longdate}|${pad:padding=5:inner=${level:uppercase=true}}|${logger}|${message}${onexception:inner=|${exception:format=tostring}${newline}${exception:format=tostring:maxInnerExceptionLevel=10}}"
      }
    },
    "rules": [
      {
        "logger": "System.*",
        "finalMinLevel": "Warn"
      },
      {
        "logger": "Microsoft.*",
        "finalMinLevel": "Warn"
      },
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "console"
      }
    ]
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "Realm": "redmist",
    "AuthServerUrl": "https://sunnywood.redmist.racing/auth/",
    "SslRequired": "external",
    "Resource": "account",
    "VerifyTokenAudience": true
  },
  "ConnectionStrings": {
    "Default": ""
  },
  "REDIS_SVC": "",
  "REDIS_PW": ""
}
